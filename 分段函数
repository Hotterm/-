#include<stdio.h>
#include<math.h>
int main()
{
	double x, y, a, b, c;
	scanf_s("%lf", &x);
	if (x > -10 && x < 4)
	{
		a = x - 2;
		printf("y=%.2f", fabs(a));
	}
	else if (x < 7 && x > 5)
	{
		b = x + 10;
		printf("y=%.2f", b);
	}
	else if (x >= 8 && x < 12)
	{
		c = pow(x, 4);
		printf("y=%.2f", c);
	}
	else
		printf("No answer.");
	return 0;

}
